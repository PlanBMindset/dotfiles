"*":
  alignment: {}
  core:
    autoHideMenuBar: true
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "monokai"
    ]
  editor:
    autoIndentOnPaste: false
    fontFamily: "Source Code Pro, Source Code Pro for Powerline"
    invisibles: {}
    preferredLineLength: 120
    showIndentGuide: true
    showInvisibles: true
    tabLength: 2
  "file-icons": {}
  "flex-tool-bar": {}
  "isotope-ui":
    backgroundGradient: true
    fontFamily: "System Default"
    fontWeight: "Regular"
  linter: {}
  "linter-eslint": {}
  "linter-phpcs": {}
  "linter-sass-lint": {}
  "linter-ui-default": {}
  "merge-conflicts": {}
  minimap:
    absoluteMode: true
  "one-dark-ui": {}
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  pigments:
    mergeColorDuplicates: true
    sortPaletteColors: "by color"
  "split-diff":
    ignoreWhitespace: true
  tabs:
    showIcons: true
  "tool-bar":
    position: "Left"
  "tree-view": {}
  welcome:
    showOnStartup: false
".html.php.text":
  editor:
    tabLength: 4
